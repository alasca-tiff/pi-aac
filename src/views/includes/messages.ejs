<% if(errors.length > 0) {%>
<div style="width: 100%;">
  <div class="alert alert-danger" style="margin: 0 auto 1rem;">
      <% errors.forEach(erro => { %>
        <%= erro %><br>
    <% });%>
  </div>
</div>
<% }%>
<% if(warnings.length > 0) {%>
  <div style="width: 100%;">
    <div class="alert alert-warning" style="margin: 0 auto 1rem;">
        <% warnings.forEach(w => { %>
          <%= w %><br>
      <% });%>
    </div>
  </div>
<% }%>
<% if(success.length > 0) {%>
  <div style="width: 100%;">
    <div class="alert alert-success" style="margin: 0 auto 1rem;">
        <% success.forEach(w => { %>
          <%= w %><br>
      <% });%>
    </div>
  </div>
<% }%>
